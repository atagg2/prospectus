file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Paniagua, Denos, Oropesa/Thermocouple Probes for Accurate Temperature Measurements in Short Duration Facilities.pdf:pdf},
institution = {ASME},
journal = {Proceedings of ASME Turbo Expo 2002},
month = jun,
