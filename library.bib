@article{Merzkirch2000,
abstract = {The Minimum Quadratic Difference (MQD) method is compared with methods conventionally used for the evaluation of PIV recordings, i.e. correlation-based evaluation with fixed interrogation windows (auto-or cross-correlation) and correlation-based tracking. The comparison is performed by studying the evaluation accuracy achieved when applying these methods to pairs of synthetic PIV recordings for which the true displacements are known. The influence of the magnitude of the particle image displacement, evaluation window size, density of particle image distribution, and particle image size on the accuracy are investigated. In all these cases the best results in terms of a statistical error are obtained with the MQD method. The superiority of the MQD method can be explained with its potential of accounting for non- uniformities in the particle image distribution and a non- uniform illumination. It is also shown that the conventional correlation-based methods may produce principal errors that are non-existent for the MQD method. The evaluation speed achievable for the MQD method by making use of the FFT is comparable to that common for the generally used auto-or cross-correlation algorithm. Finally, a quantitative explanation is given for the often observed phenomenon that PIV velocity results tend to be smaller than the true values.},
author = {Merzkirch, W. and Gui, L.},
doi = {10.1007/s003480050005},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Merzkirch, Gui/A comparative study of the MQD method and several correlation-based PIV evaluation algorithms.pdf:pdf},
issn = {0723-4864},
journal = {Experiments in Fluids},
keywords = {PIV},
mendeley-tags = {PIV},
month = jan,
pages = {36--44},
title = {{A comparative study of the MQD method and several correlation-based PIV evaluation algorithms}},
url = {http://www.springerlink.com/openurl.asp?genre=article\&id=doi:10.1007/s003480050005},
volume = {28},
year = {2000}
}
@article{Suresh2012,
annote = {Mass and energy are conserved quantities over a period, so it is not ambiguous to calculate the actual work over a period. Different methods of calculating efficiency can yield different results, in this case 10 points.
        
The unsteady pertubations can be assumed to be periodic. The increased efficiency of a PDC can be washed out by the poor turbine efficiencies./
        
Two methods are presented for calculating efficiency for unsteady flow through a turbine. The first method involves no averaging of the total pressure and bases the calculation of ideal work on an integral over the cycle. The second method utilizes a time average of the turbine inflows and outflows to calculate the ideal work.
        
The first method integrates the density, velocity, and temperature over the inlet and exit areas and over the period of the unsteady flow. For the ideal work the turbine is assumed to instantaneously expand the flow.
        
In the second method, ways to obtain averaged values for the pressure and temperature are presented. For the inflow, it is assumed that the flow is expanded from a varying inlet pressure to a steady exit pressure. For the outflow, it is assumed that the flow is being expanded from steady inlet conditions to the unsteady, but periodic, outflow conditions.
        
A numerical model of pulse detonation interaction with the turbine was created. The different methods of calculating turbine efficiency were used on the data. The first method (time resolved method) yielded an efficiency about 10 points higher than the second method (time averaged/work averaged).
        
Gains in efficiency from using pressure gain combustion might be undone by poor efficiencies in the turbine. pg 1
        
The work of Glaser et al in 2007 used a large bypass flow making the results less sensitive to changes in the work output pg 1
        
There is no averaging proceedure that is correct for all situations. pg 3
        
      },
author = {Suresh, Ambady and Hofer, Douglas C. and Tangirala, Venkat E.},
doi = {10.1115/1.4003246},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Suresh, Hofer, Tangirala/Turbine Efficiency for Unsteady, Periodic Flows.pdf:pdf},
issn = {0889504X},
journal = {Journal of Turbomachinery},
number = {3},
title = {{Turbine Efficiency for Unsteady, Periodic Flows}},
volume = {134},
year = {2012}
}
@article{Cardwell2010,
abstract = {Wall-mounted roughness features, such as ribs, are often placed along the walls of a channel to increase the convective surface area and to augment heat transfer and mixing by increasing turbulence. Depending on the relative roughness size and orientation, the ribs also have varying degrees of increased pressure losses. Designs that use ribs to promote heat transfer encompass the full range of having only a few streamwise ribs, which do not allow fully developed flow conditions, to multiple streamwise ribs, which do allow the flow to become fully developed. The majority of previous studies have focused on perturbing the geometry of the rib with little attention to the spatially and temporally varying flow characteristics and their depen- dence on the Reynolds number. A staggered rib-roughened channel study was performed using time-resolved digital particle image velocimetry (TRDPIV). Both the developing (entry region) and a fully developed region were interro- gated for three Reynolds numbers of 2,500, 10,000, and 20,000. The results indicate that the flow was more sensi- tive to Reynolds number at the inlet than within the fully developed region. Despite having a similar mean-averaged flowfield structure over the full Reynolds number range investigated, the population and distribution of coherent structures and turbulent dissipation within the fully devel- oped region were also found to be Reynolds number N. D. Cardwell  P. P. Vlachos (\&) Mechanical Engineering Department, Virginia Polytechnic Institute and State University, Blacksburg, Virginia 24061, USA e-mail: pvlachos@vt.edu K. A. Thole Mechanical and Nuclear Engineering Department, Pennsylvania State University, University Park, PA 16802, USA dependent. Exploring the time-accurate flow characteristics revealed that in addition to vortices shed from the rib shear layer, the region of the rib wake was governed by a peri- odic process of bursting of the wake vortices resulting in the intermittent ejection of the inter-rib recirculation region into the core flow. This periodic process was the driving mechanism resulting in mixing and heat transfer augmen- tation. A quadrant-splitting burst analysis was also per- formed to determine the characteristic frequency and duration of inter-rib bursting as well as the wake shedding frequency, both of which were determined to be Reynolds number dependent.},
author = {Cardwell, Nicholas D. and Vlachos, Pavlos P. and Thole, Karen a.},
doi = {10.1007/s00348-010-0993-y},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Cardwell, Vlachos, Thole/Developing and fully developed turbulent flow in ribbed channels.pdf:pdf},
issn = {0723-4864},
journal = {Experiments in Fluids},
month = oct,
number = {5},
pages = {1357--1371},
title = {{Developing and fully developed turbulent flow in ribbed channels}},
url = {http://www.springerlink.com/index/10.1007/s00348-010-0993-y},
volume = {50},
year = {2010}
}
@article{Hoke2002,
abstract = {Two methods, an ejector pump and a turbo-charger, are evaluated as a means to self-aspirate a Pulsed Detonation Engine (PDE). For the experiments pertaining to the ejector pump, a pulsed detonation engine is run on hydrogen and air at frequencies up to 40 Hz, equivalence ratios from 0.5 to 1.0, and fill fractions from 0.25 to 1.0. Flow visualization is used to determine the combination of fill fraction and equivalence ratio that successfully induced a secondary flow in the ejector pump. Pressure traces at the inlet and along the ejector pump are used to understand the performance of the ejector pump. The induced secondary flow is found to be approximately triple the primary detonation flow. Fill fraction and equivalence ratio are found to affect the performance of the ejector. High fill fractions and high equivalence ratios results in an oscillatory flow at the ejector inlet. Hydrogen and air are used as the fuel and oxidizer during the experiment with the turbo-charger also. Air flow and pressure at the exit of the compressor are used to evaluate the potential for self-aspirating the PDE. By running two detonation tubes simultaneously through the turbo-charger self- aspiration is achieved. The centrifugal style turbine and compressor of the turbo-charger showed no signs of discoloration or pitting after a 25 minute self-aspiration run where the detonation tube and turbo-charger attained thermal equilibrium. Throughout the course of the testing the turbine experienced 35K plus detonation events and reached a rotational operating speed of 80K rpm.},
address = {Reno, Nevada},
annote = {Ejector and turbo-charger are investigated as a means to self-aspirate a PDE. pg 1
        
Automotive cylinder head was used to control the flow of air into the PDE. Three different configurations of the ejector with detonator tubes were used in the experiment: a 2" diameter ejector opening, a 6" diameter ejector opening, and a 6" diameter ejector opening with a 5" diameter reflector plate at the end of the detonation tube. A "hot-wire" velocity sensor was used to measure the induced airflow. Since the "hot-wire" velocity sensor is not sensitive to direction, flow visualization was necessary to determine the direction of flow. Flow visualization was accomplished using a paper towel and a weighted string. These were then observed when the experiment was conducted. For the turbocharger experiment, two detonation tubes were used in parallel. Part of the exhaust was ducted through a turbine. The compressor exit was connected to the intake manifold of the detonation tubes. A mass flow sensor was connected to the compressor inlet.
        
At higher fill fractions and equivalence ratios the induced secondary flow was oscillatory because of reflected shock waves. However, steady secondary flow could be induced with lower fill fractions and equivalence ratios. Also, this induced secondary flow was greater than the needed flow for self-aspiration.
        
The compressor was used to self-aspirate the PDE with power derived from the PDE exhaust through the turbine. However, purge air was still supplied from the facility compressors. The self-aspirated run with the turbocharger was conducted for 25 minutes.
        
This paper shows that a PDE can be successfully self-aspirated either through an ejector pump or a turbocharger.},
author = {Hoke, John and Bradley, Royce and Stutrud, Jeff and Schauer, Fred},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Hoke et al/Integration of a Pulsed Detonation Engine with an Ejector Pump and with a Turbo-Charger as Methods to Self-Aspirate.pdf:pdf},
institution = {AIAA},
journal = {40th AIAA Aerospace Sciences Meeting \& Exhibit},
month = jan,
title = {{Integration of a Pulsed Detonation Engine with an Ejector Pump and with a Turbo-Charger as Methods to Self-Aspirate}},
year = {2002}
}
@article{Cardwell2011,
annote = {Gas turbines are not designed to handle two-phase flow, solids in an air stream. Analytical models that have been developed to model  particles that are injested into the turbine are unable to model realistic turbulent particle transport and the collisions the particles have with the walls. This paper used time resolved digital particle image velocimetry (TRDPIV) to track particles through a ribbed channel. A technique for cross correlation called robust phase correlation (RPC) is used which is an improvement on the standard cross correlation technique. Significant improvements in accuracy and vector detection were noted in the study.
        
Three different types of sand were used to seed the flow to simulate particles entering the cooling passages of a gas turbine engine. It was observed that paricles with a low Stokes number followed the flow better, as was predicted. Particles with a larger Stokes number kept their own trajectories longer and were not as affected by the motion of the fluid since their inertias were more important. The results showed that some of the assumption in analytical models are false. These assumptions include perfectly elastic collisions between particles and a particle and the wall, no particle breakup, and the particles follow the streamlines.},
author = {Cardwell, N. D. and Vlachos, P. P. and Thole, K. a.},
doi = {10.1115/1.4001187},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Cardwell, Vlachos, Thole/A Method for Identifying and Visualizing Foreign Particle Motion Using Time-Resolved Particle Tracking Velocimetry.pdf:pdf},
issn = {0889504X},
journal = {Journal of Turbomachinery},
number = {2},
title = {{A Method for Identifying and Visualizing Foreign Particle Motion Using Time-Resolved Particle Tracking Velocimetry}},
url = {http://link.aip.org/link/JOTUEI/v133/i2/p021021/s1\&Agg=doi},
