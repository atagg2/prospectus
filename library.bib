@article{Thomas2011,
abstract = {A rotating detonation engine (RDE), originally designed and built by Pratt \& Whitney – Seattle Aerosciences Center and on loan to the Air Force Research Laboratory (AFRL/RZTC) at Wright-Patterson Air Force Base (WPAFB), was rebuilt and tested. New parts were acquired, fabricated, or modified to replace those damaged or missing from initial experimentation. Originally designed for ethylene and oxygen, the RDE was modified to run on hydrogen and air while the detonation initiator uses hydrogen and oxygen. Analytical mass-flow and pressure loss equations were used to estimate combustion chamber conditions. Estimation of detonation cell size was performed via comparison to published experimental data in terms of static pressure and stoichiometry. The experimental rig was instrumented to measure feed and fill temperatures and pressures, combustion chamber temperatures and pressures, and combustion chamber wave speed. The reactant feed system was characterized in terms of mass flow and pressure. A detonation initiation device showed successful light-off of the main chamber reactants. However, cold-flow testing revealed low static pressures in the combustion chamber unfavorable to detonation propagation. Limited combustion tests showed successful and predictable light-off, but no substantial evidence of detonation combustion. Low static pressure in the combustion chamber is thought to have inhibited propagation of the detonation from the initiation device into the main chamber. Nomenclature},
address = {Orlando, Florida},
author = {Thomas, Levi M. and Schauer, Frederick R. and Hoke, John L. and Naples, Andrew},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Thomas et al/Buildup and Operation of a Rotating Detonation Engine.pdf:pdf},
institution = {AIAA},
journal = {49th AIAA Aerospace Sciences Meeting including the New Horizons Forum and Aerospace Exposition},
month = jan,
title = {{Buildup and Operation of a Rotating Detonation Engine}},
year = {2011}
}
@article{Daneshyar1969,
abstract = {Three turbines, covering a range of reaction of +20 percent to -10 percent at their blade roots, have been tested under both steady and 'pulse flow' conditions in order to assess their degree of suitablility for turbocharger application. The results have been analysed in a way which enables efficiencies to be plotted against a mean non-dimensional blade speed irrespective of the level of pulsation. This method of presentation enables the results to be used directly for design purposes and in matching calculuations. Thus the efficiency penalty due to pulse flow, the effect of pulse flow on the choice of turbine size, and the preferred degree of reaction under pulse flow may be assessed. For the present tests the range of variaton in gas speed under pulsing conditions is thought to compare in severity with that produced when a single turbocharger is connected to three cylinders of a large two-stroke marine engine. For this case the design having the highest degree of reaction is shown to give an efficiency four percentage points higher than either of the other designs. It also has a clear advantage under steady flow. The intermediate design, having zero root reaction, corresponds to the type of turbine conventionally used in turbochargers and is shown to be the one most sensitive to losses in efficiency resulting from pulsation. It appears therefore that substantial improvements could be made in turbines for future turbocharger designs by increasing the reaction. Further testing of these turbines under modified pulse conditions, with significant windage period, is advocated. The advantage of the high reaction design must be demonstrated for such conditions before a change of design reaction could be regarded as fully justifialbe.},
annote = {Increasing the reaction of turbocharcher turbines may improve performance.
        
Assuming the flow is quasi-steady in a turbocharger yields fairly accurate results for efficiency calculations at the frequencies seen in marine diesel engines. However, the accuracy decreases for higher frequencies. Also, at higher frequencies, the mass flow through the turbocharger decreases.
        
For a multi-entry radial turbine it was found that the average turbine efficiency is greater for unsteady flow than for steady flow.
        
Total-to-static efficiency is what is used to evaluate turbochargers. During the exhaust process there is a high pressure spike at the turbine inlet during blow down, a constant pressure area during exhaust, and then a dip to a low pressure after the valve has closed.
        
The pulse generator used two parallel cylinders and valves driven by cams mounted on a central shaft. It generated 444 pulses per minute. The turbine mass flow was measured using a nozzle on the turbine exhaust and measuring total pressure on the exhaust centerline. The exit static pressure was assumed to be atmospheric. The compressor mass flow was measured also using a nozzle and measuring the static pressure of the flow. The total pressure was assumed to be atmospheric.
        
The three turbines considered in the test were calibrated with a steady air flow supply. Once with a brake and once with no load. The pulse tests were carried out with a brake compressor. To increase speed the power absorption of the brake compressor was decreased.
        
The average efficiency can be plotted against the ratio of blade speed to isentropic gas speed. The efficiency can be expressed as
$\backslash$[$\backslash$eta = $\backslash$cfrac\{$\backslash$dot m\_c c\_p $\backslash$Delta T\}\{$\backslash$int\_0\^{}\{360\} $\backslash$left( $\backslash$dot m\_t c\_p T\_\{it\} $\backslash$left[ 1 - $\backslash$left( $\backslash$cfrac\{P\_\{ds\}\}\{P\_\{it\}\} $\backslash$right)\^{}\{($\backslash$gamma - 1)/ $\backslash$gamma\} $\backslash$right] $\backslash$right) $\backslash$cfrac\{$\backslash$mathrm\{d\} $\backslash$theta\}\{360\}\}$\backslash$]
where \$T\_\{it\}\$ is the inlet total temperature of the turbine, \$P\_\{ds\}\$ is the static pressure downstream of the turbine, and \$P\_\{it\}\$ is the inlet total pressure of the turbine. The numerator is based on the compressor and the denominator is based on the turbine conditions.
        
Since the instantaneous mass flow is hard to measure, the efficiency can be expressed using steady-flow capacity curves and a correction factor to calculate the mass flow rate. An average of two different methods, one using the measured inlet total temperature and the other using a measured mean total inlet temperature (use a thermocouple with a slow response time). These two methods are then averaged to obtain a corrected mass flow and a corrected efficiency. The reference gas velocity for the unsteady case was calculated by considering a steady state operation at the same mean power, average efficiency, and average mass flow.
        
A  decrease in efficiency was seen for the unsteady condition from the steady condition. The high reaction turbine showed better performance in both the steady and unsteady condition.
        
The peak pressures induced at the turbine inlet were slightly less than 3 times atmospheric pressure.},
author = {Daneshyar, H. and Edwards, K. J. and Horlock, J. H. and Janota, M. S. and Pearson, R. D. and Shaw, R.},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Daneshyar et al/A Comparison of the Performance of Three Model Axial Flow Turbines Tested Under Both Steady and Pulse Flow Conditions.pdf:pdf},
journal = {Proceedings of the Institution of Mechanical Engineers},
keywords = {Pulsed Flow,Turbocharger},
mendeley-tags = {Pulsed Flow,Turbocharger},
number = {1},
pages = {1119--1134},
title = {{A Comparison of the Performance of Three Model Axial Flow Turbines Tested Under Both Steady and Pulse Flow Conditions}},
volume = {184},
year = {1969}
}
