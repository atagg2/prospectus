}
@article{Lynch2012,
abstract = {Peak flame temperatures in aluminum particle combustion should approach the volatilization tempera- ture of the product alumina. References are divided in assigning this temperature anywhere between 3200 and 4000 K, which can provide significant uncertainty not only in numerical models for combustion but also in the interpretation of flame structure from temperature measurements. We present results in the controlled conditions of the UIUC heterogeneous shock tube of volatilization temperature, made by measuring the extinction of light by nano- and micro-alumina particles at non-resonant wavelengths at different ambient temperatures. At 10 atm, there is a sharp cutoff at 3860 K beyond which nano-par- ticles volatilize and stop extinguishing within the shock tube test time. Numerical modeling of the evap- oration rate of these particles is used to assign a volatilization temperature of 4340 K at 10 atm. Similarly, a volatilization temperature of 4260 K at 3 atm is measured. From our analysis, the best estimate for the volatilization temperature at 1 atm was 4189 ± 200 K, which is consistent with the high range of volatil- ization temperature reported in the literature.},
annote = {Maximum combustion temperature of energetic metals is limited by the volatilization temperature of the energetic material. There is a large variation in the literature on the volitilization temperature of aluminum particles from 3200 to 4000 K. The volatilization temperature of aluminum particles is measured in a shock tube. The shock can be controlled to produce the desired test temperature and pressure at the test area. A laser is used to determine whether or not the particles have volatilized. Non-volatilized aluminum particles will extiguish the laser light while volatilized aluminum particles will allow the laser light to pass through. The volatilization temperature for nano-alumina particles was measured to be 4340 K at 10 atm and 4260 K for 3 atm. The volatilization temperature for micro-alumina particles was measured to be 4340 K at 10 atm. Data was extrapolated to obtain a volatilization temperature of 4189 K at 1 atm for both particles.},
author = {Lynch, Patrick and Krier, Herman and Glumac, Nick},
doi = {10.1016/j.combustflame.2011.07.023},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Lynch, Krier, Glumac/Micro-alumina particle volatilization temperature measurements in a heterogeneous shock tube.pdf:pdf},
issn = {00102180},
journal = {Combustion and Flame},
month = feb,
number = {2},
pages = {793--801},
publisher = {The Combustion Institute.},
title = {{Micro-alumina particle volatilization temperature measurements in a heterogeneous shock tube}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0010218011002550},
volume = {159},
year = {2012}
}
@article{Cunningham1951,
abstract = {The discharge characteristics of orifices at low pressure ratios were investigated. Probe measurements of high velocity jets confirm and extend the work of Stanton. Expansion factors (Y) at pressure ratios down to r = 0.13 were measured for the flow of air in a 2-in. pipe meter and for the flow of steam in a 3-in. pipe meter. Both meters were installed in accordance with ASME specifications, and operated at a diameter ratio of 0.15. The effects of high approach velocity on the discharge characteristics of square-edged orifices at low pressure ratios were investigated; expansion factors were determined for diameter ratios from 0.2 to 0.8 over a range of pressure ratios from unity to 0.2. A linear Y-r correlation is shown to exist at low pressure ratios; a change of flow regime occurs at a pressure ratio of 0.63. A theoretical solution for super-critical flow, previously established by the author, is further compared with experimental results.},
