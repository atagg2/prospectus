@article{Dean2005,
abstract = {Pulse Detonation combustors (PDCs), as part of a hybrid PDC-turbine engine, have potential thermodynamic benefits over existing Brayton-cycle gas turbine. The form of combustion is a cyclic, controlled series of detonations. These systems apply a near or quasi-constant volume combustion process that provides both heat addition and pressure. In a hybrid PDC-turbine engine, the goal of incorporation a pulsed detonation chamber upstream of a turbine s to extract more mechanical energy in a turbine that receives the products of a repeating, pressure-rise detonation process versus the constant pressure, steady-flows available in conventional gas turbines. A rig was built to investigate PDC-turbine interactions and was operated to gather data on performance, operability, and noise levels. The rig consists of a single pused detonation combustor firig into a partial-admission, two-stage axial turbine. This paper reports findings of critical risk areas including turbine response to PDC operation, mechanical robustness, noise and system control. At a PDC operating frequency of 5 Hz, the acoustic level near the rig was approximately 3 dB higher that from the turbine operating at the same speed with steady flow input. The noise level is 28 dB lower than a PDC with no turbine downstream operating at the same frequency and discharging directly into the room. Insights into the mechanism for noise reduction were gained via imaging experiements and CFD simulation. High speed video imaging in a 2D PDC-turbine cascade configuration showed significant shock reflection from the cascade. An unsteady, reacting flow computational study showed similar shock reflection as well as a shock system that forms downstream of the cascade. Together, these results show that ashock waves are both transmitted and reflected by the turbine stages in proporitions that are dependent upon turbine stage design.},
address = {Reno-Tahoe, Nevada},
annote = {PDEs have potential because of their simplicity and efficiency. pg1275
        
PDEs can be used in subsonic, supersonic and hypersonic flight. pg 1275
        
"A detonation is a supersonic, shock-induced combustion wave that propagates through a reactive mixture. Heat release from combustion is couple to the shock and provides energy to sustain shock." pg 1275
        
Bypass air was used to cool the PDC tube and to mix with the exhaust products. Mixing with the exhaust products was necessary so that the temperature limits of the turbine were not exceeded. PDC exhaust is mixed with bypass air in a large plenum before passing through the turbine. pg 1277
        
Turbine used in this experiment was originally designed for steam. pg 1277
        
Process includes fill, detonation initiation, detonation propagation, blowdown, and purge. pg 1277
        
PDC was operated with frequencies of 5-20Hz. pg 1277
        
A 6" diameter control valve was used to control the turbine back pressure. pg 1278
        
Turbine can effectively reduce noise levels from detonation based combustion. In this particular experiment the difference was 28 dB. pg 1279
        
Sources to look at
9 comparison between PDE cycle and ramjet in a GTE
14 first jet engines used pressure gain combustion pg 1276
15 increase in power output in fired operation
16 description of PDC rig
17 description of 2D cascade
        
Summary:
        
Pulse Detonation Engines (PDEs) have potential in aircraft propulsion because of their simplicity and efficiency. Furthermore, PDEs can be used in subsonic, supersonic and hypersonic flight. This is because PDEs operate using supersonic, shock-induced combustion waves called detonations. The combustion front is coupled with the shock wave and releases energy in the form of heat that sustains the shock. The PDE process includes fill, detonation initiation, detonation propagation, blowdown, and purge.
        
In this experiment a PDE was coupled with a turbine. The PDE was operated with frequencies of 5-20Hz. A 6" diameter control valve was used to control the turbine back pressure. Bypass air was used to cool the PDE tube and to mix with the exhaust products. Mixing with the exhaust products was necessary so that the temperature limits of the turbine were not exceeded. Mixing was accomplished in a large plenum upstream of the turbine.
        
It was discovered that the presence of a turbine at the turbine exit decreased the noise levels from detonation-based combustion by 28 dB. },
author = {Dean, Anthony J.},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Dean/Operation and Noise Transmission of an Axial Turbine Driven by a Pulse Detonation Combustor.pdf:pdf},
institution = {ASME},
journal = {Proceedings of the ASME Turbo Expo 2005: Power for Land, Sea and Air},
month = jun,
pages = {1275--1284},
title = {{Operation and Noise Transmission of an Axial Turbine Driven by a Pulse Detonation Combustor}},
volume = {6},
year = {2005}
}
@techreport{Longley1992,
annote = {Distortions in the compressor can be unsteady or steady and result from flow separation during manuevers or supersonic flight.
        
The flow is assumed to have different total pressure at the inlet, but exit at the same static pressure. Thus, the system can be modeled as two different compressors pumping from a differnt total pressure to the same static pressure. This is a good assumption because there will be little circumfrencial mixing since there is very little spacing between compressor stages. Also, the pressure gradient is the greatest during the first few stages so if any redistribution occurs it will be in this range.
        
The parallel compressor model is overly simplified, but does give qualitative information about the effects of distortion.
        
With an inlet distortion, going through a diffuser the static pressure increases while the total pressure distortion remains constant. This is because the total pressure is not changing, but the static pressure changes because the flow is changing velocity in the diffuser. The exit flow from the compressor is assumed to have uniform static pressure. While the total pressure distortion will decrease through the compressor, there will still be some difference in velocity causing total pressure distortion.
        
For non-uniform flow, there is a difference in the stagnation pressure, while the static pressure may be constant. This is because of the differences in velocity.},
author = {Longley, J. P. and Greitzer, E. M.},
booktitle = {Steady and Transient Performance Prediction of Gas Turbine Engines},
file = {:C$\backslash$:/Users/Mark/Documents/Mendeley Desktop/Longley, Greitzer/Inlet Distortion Effects in Aircraft Propulsion System Integration.pdf:pdf},
institution = {AGARD},
title = {{Inlet Distortion Effects in Aircraft Propulsion System Integration}},
year = {1992}
}
